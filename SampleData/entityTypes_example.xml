<?xml version="1.0" encoding="utf-8"?>
<!--
  Example EntityType XML export from Netwrix Identity Manager.

  How to export:
    IM Admin Console > Administration > EntityTypes > select connector > Export

  Only properties with a TargetColumnIndex are mapped to C{N} columns
  in [dbo].[UR_Resources]. Properties without TargetColumnIndex
  (collection navigation properties) are skipped by the importer.

  Feed this file to:
    Initialize-Workspace -XmlPath "path\to\this_file.xml"
  or:
    Update-SQueryEntityTypes -XmlPath "path\to\this_file.xml" [-Merge]
-->
<EntityTypes>

  <!-- Example: a custom HR connector with scalar and ForeignKey properties -->
  <EntityType Identifier="Acme_Employee">
    <Property Identifier="DisplayName"    Type="String"      TargetColumnIndex="12" />
    <Property Identifier="EmployeeNumber" Type="String"      TargetColumnIndex="13" />
    <Property Identifier="StartDate"      Type="DateTime"    TargetColumnIndex="14" />
    <Property Identifier="EndDate"        Type="DateTime"    TargetColumnIndex="15" />
    <Property Identifier="Email"          Type="String"      TargetColumnIndex="16" />
    <Property Identifier="Department"     Type="ForeignKey"  TargetColumnIndex="20" />
    <Property Identifier="Manager"        Type="ForeignKey"  TargetColumnIndex="21" />
    <!-- Collection nav props have no TargetColumnIndex and are skipped -->
    <Property Identifier="Groups" />
  </EntityType>

  <!-- Example: a simple org-unit entity -->
  <EntityType Identifier="Acme_Department">
    <Property Identifier="DisplayName"    Type="String"      TargetColumnIndex="5" />
    <Property Identifier="Code"           Type="String"      TargetColumnIndex="6" />
    <Property Identifier="ParentDept"     Type="ForeignKey"  TargetColumnIndex="7" />
  </EntityType>

</EntityTypes>
