{
  "version": "1.0",
  "description": "Manual overrides applied on top of db-schema.json. Column renames, navigation property overrides, and Resource EntityType mappings. Entity aliases (tableName + alias) have moved to db-schema.json.",

  "globalColumnRenames": {
    "Description":         "Description_L1",
    "DisplayName":         "DisplayName_L1",
    "FullName":            "FullName_L1",
    "InternalDisplayName": "InternalDisplayName_L1"
  },

  "entityColumnOverrides": {
  },

  "navigationPropertyOverrides": {

    "AssignedSingleRole": {
      "Owner": { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "AssignedCompositeRole": {
      "Owner": { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "AssignedResourceType": {
      "Role":  { "targetTable": "UP_ResourceTypes", "targetEntity": "ResourceType" },
      "Owner": { "targetTable": "UR_Resources",     "targetEntity": "Resource"     }
    },

    "UserActivityInstance": {
      "AssignedTo": { "targetTable": "UR_Resources", "targetEntity": "Resource" },
      "Performer":  { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "WorkflowInstance": {
      "ActivityInstances":      { "targetTable": "UW_ActivityInstances",      "targetEntity": "UserActivityInstance", "localKey": "Id", "foreignKey": "WorkflowInstance_Id" },
      "AssignedSingleRoles":    { "targetTable": "UP_AssignedSingleRoles",    "targetEntity": "AssignedSingleRole",   "localKey": "Id", "foreignKey": "WorkflowInstance_Id" },
      "AssignedCompositeRoles": { "targetTable": "UP_AssignedCompositeRoles", "targetEntity": "AssignedCompositeRole", "localKey": "Id", "foreignKey": "WorkflowInstance_Id" },
      "AssignedResourceTypes":  { "targetTable": "UP_AssignedResourceTypes",  "targetEntity": "AssignedResourceType", "localKey": "Id", "foreignKey": "WorkflowInstance_Id" },
      "Directory_FR_User":          { "targetTable": "UR_Resources", "targetEntity": "Resource", "localKey": "Owner_Id" },
      "Workflow_Directory_FR_User": { "targetTable": "UR_Resources", "targetEntity": "Resource", "localKey": "Owner_Id" }
    },

    "Workflow": {
      "Activities": { "targetTable": "UW_Activities", "targetEntity": "Activity", "localKey": "Id", "foreignKey": "Workflow_Id" }
    },

    "Policy": {
      "SimulationPolicy":   { "targetTable": "UP_PolicySimulations", "targetEntity": "SimulationPolicy", "localKey": "PolicySimulation_Id"   },
      "ProvisioningPolicy": { "targetTable": "UP_Policies",          "targetEntity": "Policy",           "localKey": "PolicyProvisioning_Id" }
    },

    "CompositeRoleRule": {
      "CreatedBy": { "targetTable": "UR_Resources", "targetEntity": "Resource" },
      "ChangedBy": { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "ResourceNavigationRule": {
      "Resource": { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "ResourceTypeRule": {
      "Role": { "targetTable": "UP_ResourceTypes", "targetEntity": "ResourceType" }
    },

    "ResourceType": {
      "SecondaryCategories": { "targetTable": "UP_ResourceTypesCategories", "targetEntity": "ResourceTypesCategory", "localKey": "Id", "foreignKey": "ResourceType_Id" }
    },

    "Job": {
      "LastJobInstance": { "targetTable": "UJ_JobInstances", "targetEntity": "JobInstance" }
    },

    "JobInstance": {
      "User": { "targetTable": "UR_Resources", "targetEntity": "Resource" }
    },

    "Connector": {
      "Connections":        { "targetTable": "UC_Connections",        "targetEntity": "Connection",        "localKey": "Id", "foreignKey": "Connector_Id" },
      "EntityTypeMappings": { "targetTable": "UC_EntityTypeMappings", "targetEntity": "EntityTypeMapping", "localKey": "Id", "foreignKey": "Connector_Id" }
    },

    "EntityTypeMapping": {
      "EntityType": { "targetTable": "UM_EntityTypes", "targetEntity": "EntityType" }
    }
  },

  "resourceEntityTypes": {
  }
}
